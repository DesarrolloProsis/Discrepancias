<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=.9, shrink-to-fit=no">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <link href="https://kendo.cdn.telerik.com/2018.3.1017/styles/kendo.common-bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://kendo.cdn.telerik.com/2018.3.1017/styles/kendo.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/css/anim.css" rel="stylesheet" />
    <link href="~/Scripts/css/estilos.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    <script src="https://kendo.cdn.telerik.com/2018.3.1017/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2018.3.1017/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2018.3.1017/js/kendo.aspnetmvc.min.js"></script>
    @RenderSection("scripts", required: false)

</head>
<body>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top">
        <a class="navbar-brand" href="#">
            <img src="~/Content/img/logo.png" alt="logo">
        </a>

        <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#collapsibleNavbar">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="collapsibleNavbar">
            <ul class="navbar-nav ml-auto ">
                <li class="nav-item">
                    @Html.ActionLink("Discrepancias", "Index", "Home", new { area = "" }, new { @class = " nav-link" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Rango De Fechas", "IndexSecundario", "Home", new { area = "" }, new { @class = " nav-link" })
                </li>
                <li class="nav-item">
                    @*@Html.ActionLink("Graficas", "IndexGraficas", "Home", new { area = "" }, new { @class = " nav-link" })*@
                </li>
            </ul>
        </div>
    </nav>
    <div class="container ">
        @RenderBody()

        <div class="footer">
            <p class="footertext">©2018 Proyectos y Sistemas Informáticos S.A. de C.V. Todos los Derechos Reservados.</p>
        </div>
    </div>
</body>
</html>
